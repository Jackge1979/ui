	"watch": {
		"build-cmf": {
			"patterns": [
				"packages/cmf/src/**/*"
			],
			"extensions": "js"
		},
		"build-components": {
			"patterns": [
				"packages/components/src/**/*"
			],
			"extensions": "js,css,scss,svg"
		},
		"build-containers": {
			"patterns": [
				"packages/containers/src/**/*"
			],
			"extensions": "js,css,scss,svg"
		},
		"build-forms": {
			"patterns": [
				"packages/forms/src/**/*"
			],
			"extensions": "js,scss"
		},
		"build-icons": {
			"patterns": [
				"packages/icons/src/svg/*"
			],
			"extensions": "svg"
		},
		"build-theme": {
			"patterns": [
				"packages/theme/src/**/*"
			],
			"extensions": "css,scss"
		}
	},
